{
  "name": "@kiwiproject/kiwi-test-js",
  "version": "0.0.0-semantically-released",
  "description": "kiwi-test-js is a test utility library. Most of these utilities are ports from the Java Kiwi-test library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiwiproject/kiwi-test-js.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsc",
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(ts|js|json)\"",
    "lint": "npx eslint --fix src",
    "test": "npx jest --coverage --collectCoverageFrom=src/**/*.ts"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.ts"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "9.2.0",
    "@jest/globals": "30.2.0",
    "@kiwiproject/kiwi-js": "1.0.0",
    "@testcontainers/elasticsearch": "11.11.0",
    "@testcontainers/postgresql": "11.11.0",
    "convert": "6.0.0",
    "jest": "30.2.0",
    "mongodb": "7.0.0",
    "pg": "8.17.2",
    "redis": "5.10.0",
    "testcontainers": "11.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.6",
    "semantic-release": "25.0.2",
    "@babel/preset-env": "7.28.6",
    "@babel/preset-typescript": "7.28.5",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.9",
    "@types/pg": "8.16.0",
    "babel-jest": "30.2.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "globals": "17.0.0",
    "prettier": "3.8.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0"
  }
}
