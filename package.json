{
  "name": "@kiwiproject/kiwi-test-js",
  "version": "0.6.0",
  "description": "kiwi-test-js is a test utility library. Most of these utilities are ports from the Java Kiwi-test library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(ts|js|json)\"",
    "lint": "npx eslint --fix src",
    "test": "npx jest --coverage --collectCoverageFrom=src/**/*.ts"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.ts"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "9.0.3",
    "@jest/globals": "30.0.4",
    "@kiwiproject/kiwi-js": "0.12.0",
    "@testcontainers/elasticsearch": "11.3.1",
    "@testcontainers/postgresql": "11.3.1",
    "convert": "5.12.0",
    "jest": "30.0.4",
    "mongodb": "6.17.0",
    "pg": "8.16.3",
    "redis": "5.6.0",
    "testcontainers": "11.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/preset-env": "7.28.0",
    "@babel/preset-typescript": "7.27.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.31.0",
    "@types/node": "24.0.15",
    "@types/pg": "8.15.4",
    "babel-jest": "30.0.4",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.37.0"
  }
}
