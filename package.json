{
  "name": "@kiwiproject/kiwi-test-js",
  "version": "0.1.0",
  "description": "kiwi-test-js is a test utility library. Most of these utilities are ports from the Java Kiwi-test library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage --collectCoverageFrom=src/**/*.ts --passWithNoTests"
  },
  "jest": {
    "testPathIgnorePatterns": ["<rootDir>/__tests__/__utils__", "<rootDir>/node_modules/"]
  },
  "dependencies": {
    "jest": "29.6.2"
  }
}
